module ram_test_vlg_vec_tst();
// constants                                           
// general purpose registers
reg [7:0] address;
reg CLOCK_50;
reg [7:0] data_in;
reg writes;
// wires                                               
wire [7:0] HEX0;
wire [7:0] HEX1;

wire sampler;                             

// assign statements (if any)                          
ram_test i1 (
// port map - connection between master ports and signals/registers   
	.address(address),
	.CLOCK_50(CLOCK_50),
	.data_in(data_in),
	.HEX0(HEX0),
	.HEX1(HEX1),
	.writes(writes)
);
// address[ 7 ]
initial
begin
	address[7] = 1'b1;
end 
// address[ 6 ]
initial
begin
	address[6] = 1'b0;
end 
// address[ 5 ]
initial
begin
	address[5] = 1'b0;
end 
// address[ 4 ]
initial
begin
	address[4] = 1'b0;
end 
// address[ 3 ]
initial
begin
	address[3] = 1'b0;
	address[3] = #900000 1'b1;
end 
// address[ 2 ]
initial
begin
	address[2] = 1'b0;
	address[2] = #300000 1'b1;
	address[2] = #600000 1'b0;
end 
// address[ 1 ]
initial
begin
	address[1] = 1'b1;
	address[1] = #300000 1'b0;
	address[1] = #300000 1'b1;
	address[1] = #300000 1'b0;
end 
// address[ 0 ]
initial
begin
	repeat(3)
	begin
		address[0] = 1'b0;
		address[0] = #150000 1'b1;
		# 150000;
	end
	address[0] = 1'b0;
end 
// data_in[ 7 ]
initial
begin
	data_in[7] = 1'b1;
end 
// data_in[ 6 ]
initial
begin
	data_in[6] = 1'b0;
end 
// data_in[ 5 ]
initial
begin
	data_in[5] = 1'b0;
end 
// data_in[ 4 ]
initial
begin
	data_in[4] = 1'b1;
end 
// data_in[ 3 ]
initial
begin
	data_in[3] = 1'b0;
	data_in[3] = #300000 1'b1;
end 
// data_in[ 2 ]
initial
begin
	data_in[2] = 1'b1;
	data_in[2] = #300000 1'b0;
	data_in[2] = #600000 1'b1;
end 
// data_in[ 1 ]
initial
begin
	data_in[1] = 1'b1;
	data_in[1] = #300000 1'b0;
	data_in[1] = #300000 1'b1;
	data_in[1] = #300000 1'b0;
end 
// data_in[ 0 ]
initial
begin
	repeat(3)
	begin
		data_in[0] = 1'b0;
		data_in[0] = #150000 1'b1;
		# 150000;
	end
	data_in[0] = 1'b0;
end 

// CLOCK_50
always
begin
	CLOCK_50 = 1'b0;
	CLOCK_50 = #50000 1'b1;
	#50000;
end 

// writes
initial
begin
	writes = 1'b0;
	writes = #10000 1'b1;
	writes = #90000 1'b0;
end 

ram_test_vlg_sample_tst tb_sample (
	.address(address),
	.CLOCK_50(CLOCK_50),
	.data_in(data_in),
	.writes(writes),
	.sampler_tx(sampler)
);

ram_test_vlg_check_tst tb_out(
	.HEX0(HEX0),
	.HEX1(HEX1),
	.sampler_rx(sampler)
);
endmodule

